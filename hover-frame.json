{
  "id": "com.custom.hover.frame",
  "version": "1.0.0",
  "name": "Hover Frame",
  "description": "A transparent container that adds hover effects to charts",
  "newInstancePrefix": "HoverFrame",
  "icon": "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR42mNk+M9QDwADhgGAWjR9awAAAABJRU5ErkJggg==",
