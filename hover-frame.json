    {
      "kind": "main",
      "tag": "com-custom-hover-frame",
      "url": "https://YOUR-CDN-URL/hover-frame.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "borderColor": {
      "type": "string",
